{
  "manifest_version": 3,
  "name": "WebQueue",
  "version": "0.1.0.0",
  "description": "Save the current website to your personalized queue",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
      
    }
  ],
  

  
  "action" :{
    "default_popup" : "popup.html"   
  }
}